public class Looper {
    public static void main (String[] args) {
	System.out.println( randArray());
    }
    public static int[] randArray () {
	int numInt = 2147483647;
	int[] randArray;
	randArray = new int[numInt];
	for (int i = -2147483647; i < 2147483647; i++) {
	    int randNum = ((int) (Math.random()* numInt) -  numInt/2);
	    randArray[i] = randNum;
	} return randArray;
    }
}

